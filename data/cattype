# Cattype data file for Forward to Libraries Service
# Ed. by John Mark Ockerbloom, University of Pennsylvania 
#  (contact details at everybodyslibraries.com)
# This data file released to the world as CC0 
#   (though appropriate attribution appreciated)
# See docs file for documentation, libraries file for usage of this catalog data
# Last updated 2022-01-11

CATTYPE 360search
SUBURL ${BASEURL}/results?SS_LibHash=${LCODE}&action=start&dbIDList=${DBID}&field=subject&term=${ARG}
AUTURL ${BASEURL}/results?SS_LibHash=${LCODE}&action=start&dbIDList=${DBID}&field=author&term=${ARG}
TITURL ${BASEURL}/results?SS_LibHash=${LCODE}&action=start&dbIDList=${DBID}&field=title&term=${ARG}
KEYURL ${BASEURL}/results?SS_LibHash=${LCODE}&action=start&dbIDList=${DBID}&field=keyword&term=${ARG}

CATTYPE aleph1
TINDEX TTL
SINDEX SUB
SUBURL ${BASEURL}/F/?func=find-b&find_code=${SINDEX}&local_base=${LCODE}&request=${ARG}
AUTURL ${BASEURL}/F/?func=find-b&find_code=AUT&local_base=${LCODE}&request=${ARG}
TITURL ${BASEURL}/F/?func=find-b&find_code=${TINDEX}&local_base=${LCODE}&request=${ARG:A}
KEYURL ${BASEURL}/F/?func=find-b&find_code=WRD&local_base=${LCODE}&request=${ARG}

CATTYPE alephcuny
SINDEX SCAN_SUL
TINDEX TTL
SUBURL ${BASEURL}/F/?func=find-e&find_scan_code=${SINDEX}&local_base=${LCODE}&request=${ARG}
AUTURL ${BASEURL}/F/?func=find-b&find_code=AUT&local_base=${LCODE}&request=${ARG}
TITURL ${BASEURL}/F/?func=find-b&find_code=${TINDEX}&local_base=${LCODE}&request=${ARG:A}
KEYURL ${BASEURL}/F/?func=find-b&find_code=WRD&local_base=${LCODE}&request=${ARG}

# We may phase this out in favor of the alexandria6 below,
# since I know of no OPACs preferring this syntax, & other more flexible
CATTYPE alexandria
SUBURL ${BASEURL}&results=SearchType0%3DKeyword%26SearchField0%3DSubjects%3A${ARG:NK}%26Interface%3Dsimple%26localCollection%3D1
TITURL ${BASEURL}&results=SearchType0%3DKeyword%26SearchField0%3DTitle%3A${ARG}%26Interface%3Dsimple%26localCollection%3D1
AUTURL ${BASEURL}&results=SearchType0%3DKeyword%26SearchField0%3DAuthor%3A${ARG:NK}%26Interface%3Dsimple%26localCollection%3D1
ATIURL ${BASEURL}&results=SearchType0%3DKeyword%26SearchField0%3D${AUTHOR:SNK}%20${TITLE:NKA}%26Interface%3Dsimple%26localCollection%3D1
KEYURL ${BASEURL}&results=SearchType0%3DKeyword%26SearchField0%3D${ARG:NK}%26Interface%3Dsimple%26localCollection%3D1

# Syntax that seems to be preferred by Alexandria 6, possibly other versions
CATTYPE alexandria6
AINDEX Author
SINDEX Subjects
KINDEX Keyword
TINDEX Title
LCODE 0
SUBURL ${BASEURL}results=SearchType0%3D${SINDEX}%26SearchField0%3D${ARG:NK}%26Interface%3Dsimple%26localCollection%3D${LCODE}
AUTURL ${BASEURL}results=SearchType0%3D${AINDEX}%26SearchField0%3D${ARG:SNK}%26Interface%3Dsimple%26localCollection%3D${LCODE}
TITURL ${BASEURL}results=SearchType0%3D${TINDEX}%26SearchField0%3D${ARG:NK}%26Interface%3Dsimple%26localCollection%3D${LCODE}
KEYURL ${BASEURL}results=SearchType0%3D${KINDEX}%26SearchField0%3D${ARG:NK}%26Interface%3Dsimple%26localCollection%3D${LCODE}

# Biblionix makes apollo, supports basic OpenSearch (not sure if other options)
# libraries also may have minimal cataloging
CATTYPE apollo
KEYURL ${BASEURL}?search=${ARG:N}

# Here's an attempt to do fielded search
CATTYPE apollofielded
SUBURL ${BASEURL}?search-scope=subject&search=${ARG:N}
KEYURL ${BASEURL}?search-scope=keyword&search=${ARG:N}

CATTYPE aquabrowser
SINDEX subjects
KINDEX ex-Everything-1.0
TINDEX title
SUBURL ${BASEURL}/?q=${SINDEX}:"${ARG}"+
TITURL ${BASEURL}/?q=${TINDEX}:"${ARG}"+
AUTURL ${BASEURL}/?q=author:"${ARG}"+
ATIURL ${BASEURL}/?q=${TINDEX}:"${TITLE}"+author:"${AUTHOR}"+
KEYURL ${BASEURL}/?q=${ARG}+

CATTYPE aquabrowser-profile
SINDEX subjects
KINDEX ex-Everything-1.0
TINDEX title
SUBURL ${BASEURL}/?c_profile=${LCODE}&q=${SINDEX}:"${ARG}"+
TITURL ${BASEURL}/?c_profile=${LCODE}&q=${TINDEX}:"${ARG}"+
AUTURL ${BASEURL}/?c_profile=${LCODE}&q=author:"${ARG}"+
ATIURL ${BASEURL}/?c_profile=${LCODE}&q=${TINDEX}:"${TITLE}"+author:"${AUTHOR}"+
KEYURL ${BASEURL}/?c_profile=${LCODE}&q=${ARG}+

# This doesn't seem to be working cold with current Arena installations
CATTYPE arena
SUBURL ${BASEURL}/search?p_p_state=normal&p_p_lifecycle=1&p_p_action=1&p_p_id=searchResult_WAR_arenaportlets&&p_p_mode=view&search_item_no=0&search_type=solr&search_query=subject:(${ARG})
AUTURL ${BASEURL}/search?p_p_state=normal&p_p_lifecycle=1&p_p_action=1&p_p_id=searchResult_WAR_arenaportlets&&p_p_mode=view&search_item_no=0&search_type=solr&search_query=author:(${ARG})
TITURL ${BASEURL}/search?p_p_state=normal&p_p_lifecycle=1&p_p_action=1&p_p_id=searchResult_WAR_arenaportlets&&p_p_mode=view&search_item_no=0&search_type=solr&search_query=title:(${ARG})
KEYURL ${BASEURL}/search?p_p_state=normal&p_p_lifecycle=1&p_p_action=1&p_p_id=searchResult_WAR_arenaportlets&&p_p_mode=view&search_item_no=0&search_type=solr&search_query=${ARG}

# This might work better for newer arena installations
# For some reason, many libraries seem to use the same code
CATTYPE arenacode
LCODE 687834046
SUBURL ${BASEURL}/search?p_p_id=searchResult_WAR_arenaportlets&p_p_lifecycle=1&p_p_state=normal&p_p_mode=view&p_r_p_${LCODE}_search_query=subject:(${ARG})
AUTURL ${BASEURL}/search?p_p_id=searchResult_WAR_arenaportlets&p_p_lifecycle=1&p_p_state=normal&p_p_mode=view&p_r_p_${LCODE}_search_query=author:(${ARG})
TITURL ${BASEURL}/search?p_p_id=searchResult_WAR_arenaportlets&p_p_lifecycle=1&p_p_state=normal&p_p_mode=view&p_r_p_${LCODE}_search_query=title:(${ARG})
KEYURL ${BASEURL}/search?p_p_id=searchResult_WAR_arenaportlets&p_p_lifecycle=1&p_p_state=normal&p_p_mode=view&p_r_p_${LCODE}_search_query=${ARG}

# We'll assume generic parameters are in the baseurl, since we don't
# know how much they vary between catalogs
CATTYPE atcult
# leaving off subject b/c with atcult libraries keyword search works better
# SUBURL ${BASEURL}&o=score&h=sub_bc&q=${ARG}
TITURL ${BASEURL}&o=title&h=title_bc&q=${ARG}
AUTURL ${BASEURL}&o=author&h=author_bc&q=${ARG}
KEYURL ${BASEURL}&o=score&h=any_bc&q=${ARG}

CATTYPE atriuum
SUBURL ${BASEURL}ExpertSearch?ST0=S&view=Bibliographic&SF0=${ARG}
AUTURL ${BASEURL}ExpertSearch?ST0=N&view=Bibliographic&SF0=${ARG}
TITURL ${BASEURL}ExpertSearch?ST0=T&view=Bibliographic&SF0=${ARG}
ATIURL ${BASEURL}ExpertSearch?ST0=N&view=Bibliographic&SF0=${AUTHOR}&ST1=T&SF1=${TITLE}
KEYURL ${BASEURL}ExpertSearch?ST0=Z&view=Bibliographic&SF0=${ARG}

CATTYPE bibliocommons
SUBURL ${BASEURL}/search?t=subject&q=${ARG}
AUTURL ${BASEURL}/search?t=author&q=${ARG}
TITURL ${BASEURL}/search?t=title&q=${ARG}
# ATIURL ${BASEURL}/search?custom_query=contributor:(${AUTHOR})+-title:(${TITLE:A})
KEYURL ${BASEURL}/search?t=smart&search_category=keyword&q=${ARG}

CATTYPE blacklight
AINDEX author
SINDEX subject_terms
SUBURL ${BASEURL}/?q=${ARG}&search_field=${SINDEX}
AUTURL ${BASEURL}/?q=${ARG}&search_field=${AINDEX}
TITURL ${BASEURL}/?q=${ARG:A}&search_field=title
KEYURL ${BASEURL}/?q=${ARG}
DEFAULT DOMAIN

# some blacklight installations work best when they set special params
CATTYPE blacklight-custom
AINDEX author
SINDEX subject
SUBURL ${BASEURL}q=${ARG}&search_field=${SINDEX}
AUTURL ${BASEURL}q=${ARG}&search_field=${AINDEX}
TITURL ${BASEURL}q=${ARG:A}&search_field=title
KEYURL ${BASEURL}q=${ARG}

# Wisconsin seems to like the search type up front; not sure if other BLs do
CATTYPE blacklight-typeatfront
AINDEX author
SINDEX subject
TINDEX title
SUBURL ${BASEURL}/${SINDEX}?q=${ARG}
AUTURL ${BASEURL}/${AINDEX}?q=${ARG}
TITURL ${BASEURL}/${TINDEX}?q=${ARG}
KEYURL ${BASEURL}?q=${ARG}

CATTYPE carlweb
SUBURL ${BASEURL}/jsp/DoSearch?databaseID=${DBID}&index=sb/&terms=${ARG}
AUTURL ${BASEURL}/jsp/DoSearch?databaseID=${DBID}&index=n&terms=${ARG}
KEYURL ${BASEURL}/jsp/DoSearch?databaseID=${DBID}&selection=Search&index=w&terms=${ARG}

# A VTLS catalog; keyword search string found via Springshare
CATTYPE chameleon
KEYURL ${BASEURL}?function=INITREQ&DEFAULT&search=KEYWORD&u1=1035&t1=${ARG}
DEFAULT DOMAIN

# Another VTLS catalog
CATTYPE chamo
SUBURL ${BASEURL}/search/query?field_1=s&term_1=${ARG}
AUTURL ${BASEURL}/search/query?field_1=a&term_1=${ARG}
TITURL ${BASEURL}/search/query?field_1=t&term_1=${ARG}
ATIURL ${BASEURL}/search/query?field_1=a&term_1=${AUTHOR}&field_2=t&term_2=${TITLE}
KEYURL ${BASEURL}/search/query?term_1=${ARG}

# Corvina appears to be used by multiple Hungarian libraries
#  but doesn't seem to have a general keyword search (author is default?)
CATTYPE corvina
AUTURL ${BASEURL}?action=find&index0=AUTH&text0=${ARG:SNK}
TITURL ${BASEURL}?action=find&index0=TITL&text0=${ARG}
SUBURL ${BASEURL}?action=find&index0=SUBJ&text0=${ARG}
KEYURL ${BASEURL}?action=find&index0=AUTH&text0=${ARG:SNK}

# Destiny sites hosted by Follett need a CONTEXT argument
# non-hosted sites might use different syntax; see Springshare guide
CATTYPE destinyhosted
SUBURL ${BASEURL}/cataloging/servlet/handlebasicsearchform.do?searchType=subject&redisplay=false&includeLibrary=true&siteTypeID=-2&keywordText=${ARG}&site=${LCODE}&context=${CONTEXT}&redisplay=false
AUTURL ${BASEURL}/cataloging/servlet/handlebasicsearchform.do?searchType=author&redisplay=false&includeLibrary=true&siteTypeID=-2&keywordText=${ARG}&site=${LCODE}&context=${CONTEXT}&redisplay=false
TITURL ${BASEURL}/cataloging/servlet/handlebasicsearchform.do?searchType=title&redisplay=false&includeLibrary=true&siteTypeID=-2&keywordText=${ARG}&site=${LCODE}&context=${CONTEXT}&redisplay=false
KEYURL ${BASEURL}/cataloging/servlet/handlebasicsearchform.do?searchType=keyword&redisplay=false&includeLibrary=true&siteTypeID=-2&keywordText=${ARG}&site=${LCODE}&context=${CONTEXT}&redisplay=false

# this doesn't seem to work on the ILS I tried it on
CATTYPE dra
KEYURL ${BASEURL}/tramp2.exe/do_keyword_search/guest&query={${ARG}}

# EDS based on http://support.epnet.com/knowledge_base/detail.php?id=2747
#  and some reverse-engineering.  Guest access needs to be enabled to
#  make this work for a given library.
# (We also need a custid to route it to a particular library's
#  collection; I have not yet been able to verify if this overrides
#  routing to local collection search at libraries that already have EDS)
CATTYPE eds
BASEURL http://search.ebscohost.com/login.aspx?direct=true
GID main
PROFILE eds
AUTHTYPE ip,guest
SUBURL ${BASEURL}&authtype=${AUTHTYPE}&custid=${LCODE}&groupid=${GID}&profile=${PROFILE}&bquery=SU%20(${ARG})
AUTURL ${BASEURL}&authtype=${AUTHTYPE}&custid=${LCODE}&groupid=${GID}&profile=${PROFILE}&bquery=AU%20(${ARG:S})
TITURL ${BASEURL}&authtype=${AUTHTYPE}&custid=${LCODE}&groupid=${GID}&profile=${PROFILE}&bquery=TI%20(${ARG})
KEYURL ${BASEURL}&authtype=${AUTHTYPE}&custid=${LCODE}&groupid=${GID}&profile=${PROFILE}&bquery=(${ARG})

CATTYPE encore
SUBURL ${BASEURL}/search/C__Sd%3A%28${ARG}%29__Orightresult__U1?lang=eng&suite=${SKIN}
AUTURL ${BASEURL}/search/C__Sa%3A%28${ARG}%29__Orightresult__U1?lang=eng&suite=${SKIN}
KEYURL ${BASEURL}/search/C__S%3A%28${ARG}%29__Orightresult__U1?lang=eng&suite=${SKIN}

CATTYPE encorehosted
SUBURL ${BASEURL}/search*eng/X?SEARCH=d:(${ARG:N})&SORT=D
AUTURL ${BASEURL}/search*eng/X?SEARCH=a:(${ARG:N})&SORT=D
TITURL ${BASEURL}/search*eng/X?SEARCH=t:(${ARG:N})&SORT=D
ATIURL ${BASEURL}/search*eng/X?SEARCH=t:(${TITLE:N})%20and%20a:(${AUTHOR:N})&SORT=D
KEYURL ${BASEURL}/search*eng/X?SEARCH=(${ARG:N})&SORT=D

CATTYPE encorekey
KEYURL ${BASEURL}/search/C|S${ARG}|Orightresult|U1?lang=eng&suite=${SKIN}

CATTYPE endeca
SINDEX Subject
AINDEX Author
TINDEX Title
SUBURL ${BASEURL}?N=0&Ntk=${SINDEX}&Ntt=${ARG}&Nty=1&D=${ARG}&Ntx=mode+matchall&Dx=mode+matchall
TITURL ${BASEURL}?N=0&Ntk=${TINDEX}&Ntt=${ARG}&Nty=1&D=${ARG}&Ntx=mode+matchall&Dx=mode+matchall
AUTURL ${BASEURL}?N=0&Ntk=${AINDEX}&Ntt=${ARG}&Nty=1&D=${ARG}&Ntx=mode+matchall&Dx=mode+matchall
KEYURL ${BASEURL}?N=0&Ntk=${KINDEX}&Ntt=${ARG}&Nty=1&D=${ARG}&Ntx=mode+matchall&Dx=mode+matchall
DEFAULT DOMAIN

CATTYPE eosweb
SUBURL ${BASEURL}?IsFirstDisplay=FALSE&GoPressed=TRUE&ShowOptions=FALSE&selectField1=SU&txtSearch1=${ARG}
TITURL ${BASEURL}?IsFirstDisplay=FALSE&GoPressed=TRUE&ShowOptions=FALSE&selectField1=AU&txtSearch1=${ARG}
TITURL ${BASEURL}?IsFirstDisplay=FALSE&GoPressed=TRUE&ShowOptions=FALSE&selectField1=TI&txtSearch1=${ARG}
KEYURL ${BASEURL}?IsFirstDisplay=FALSE&GoPressed=TRUE&ShowOptions=FALSE&selectField1=KW&txtSearch1=${ARG}

CATTYPE evergreen
SUBURL ${BASEURL}/skin/default/xml/rresult.xml?rt=keyword&l=${LCODE}&tp=keyword&t=%20subject:${ARG}
AUTURL ${BASEURL}/skin/default/xml/rresult.xml?rt=keyword&l=${LCODE}&tp=keyword&t=%20author:${ARG}
TITURL ${BASEURL}/skin/default/xml/rresult.xml?rt=keyword&l=${LCODE}&tp=keyword&t=%20title:${ARG}
KEYURL ${BASEURL}/skin/default/xml/rresult.xml?rt=keyword&l=${LCODE}&tp=keyword&t=${ARG}
DEFAULT DOMAIN

CATTYPE evergreen-dynamic
SUBURL ${BASEURL}/results?locg=${LCODE}&qtype=subject&query=${ARG}
AUTURL ${BASEURL}/results?locg=${LCODE}&qtype=author&query=${ARG}
TITURL ${BASEURL}/results?locg=${LCODE}&qtype=title&query=${ARG}
KEYURL ${BASEURL}/results?locg=${LCODE}&qtype=keyword&query=${ARG}
DEFAULT DOMAIN

# same as evergreen, but with more control over URL search term,s
CATTYPE evergreen-general
SUBURL ${BASEURL}qtype=subject&query=${ARG}
AUTURL ${BASEURL}qtype=author&query=${ARG}
TITURL ${BASEURL}qtype=title&query=${ARG}
KEYURL ${BASEURL}qtype=keyword&query=${ARG}
DEFAULT DOMAIN

# Explorit often has "powered by Deep Web technologies" note
CATTYPE explorit
AUTURL ${BASEURL}/result-list/author:${ARG}/
TITURL ${BASEURL}/result-list/title:${ARG}/
KEYURL ${BASEURL}/result-list/fullRecord:${ARG}/

# Florida Center for Library Automation catalog, used by some Florida libs
CATTYPE fcla
SUBURL ${BASEURL}?ix=su&st=${ARG}
AUTURL ${BASEURL}?ix=au&st=${ARG}
TITURL ${BASEURL}?ix=ti&st=${ARG}
KEYURL ${BASEURL}?ix=kw&st=${ARG}

# IDS project (hosted; seems to be mostly college libraries in NY)
CATTYPE ids
BASEURL http://search.idsproject.org/index.php
SUBURL ${BASEURL}?oclc_symbol=${LCODE}&view=ids&search_scope=my_library&query_index[0]=su&query_value[0]=${ARG}
AUTURL ${BASEURL}?oclc_symbol=${LCODE}&view=ids&search_scope=my_library&query_index[0]=au&query_value[0]=${ARG}
TITURL ${BASEURL}?oclc_symbol=${LCODE}&view=ids&search_scope=my_library&query_index[0]=ti&query_value[0]=${ARG}
KEYURL ${BASEURL}?oclc_symbol=${LCODE}&view=ids&search_scope=my_library&query_index[0]=kw&query_value[0]=${ARG}

# just managed to get this to work; so far general keyword searches only
CATTYPE iguana
KEYURL ${BASEURL}/iguana/www.main.cls?sUrl=search#searchTerm=${ARG}

# this we'll use for members of webpacpro consortia
CATTYPE iii-consortium-member
SUBURL ${BASEURL}/search/Y?searchscope=${LCODE}&searchtype=d&searcharg=${ARG:N}
AUTURL ${BASEURL}/search/Y?searchscope=${LCODE}&searchtype=a&searcharg=${ARG:N}
TITURL ${BASEURL}/search/Y?searchscope=${LCODE}&searchtype=t&searcharg=${ARG:N}
KEYURL ${BASEURL}/search/Y?searchscope=${LCODE}&searchtype=X&searcharg=${ARG:N}

# For now, we'll just do keyword search; hope to figure out others eventually
CATTYPE insignia
KEYURL ${BASEURL}?l=All&action=simple&t_input=Keywords&t=Keywords&k=${ARG}

# aka Horizon
CATTYPE ipac20
KINDEX .GW
TINDEX .TW
AINDEX .AW
SINDEX PSUBJ
SUBURL ${BASEURL}/ipac20/ipac.jsp?profile=${PROFILE}&index=${SINDEX}&term=${ARG}
AUTURL ${BASEURL}/ipac20/ipac.jsp?profile=${PROFILE}&index=${AINDEX}&term=${ARG}
TITURL ${BASEURL}/ipac20/ipac.jsp?profile=${PROFILE}&index=${TINDEX}&term=${ARG}
KEYURL ${BASEURL}/ipac20/ipac.jsp?profile=${PROFILE}&index=${KINDEX}&term=${ARG}

CATTYPE innopac
SUBURL ${BASEURL}/search/?searchtype=d&searcharg=${ARG:N}
AUTURL ${BASEURL}/search/?searchtype=a&searcharg=${ARG:N}
TITURL ${BASEURL}/search/?searchtype=t&searcharg=${ARG:N}
KEYURL ${BASEURL}/search/?searchtype=X&searcharg=${ARG:N}

CATTYPE koha
SUBURL ${BASEURL}/cgi-bin/koha/opac-search.pl?idx=su&q=${ARG}
AUTURL ${BASEURL}/cgi-bin/koha/opac-search.pl?idx=au&q=${ARG}
TITURL ${BASEURL}/cgi-bin/koha/opac-search.pl?idx=ti&q=${ARG}
KEYURL ${BASEURL}/cgi-bin/koha/opac-search.pl?q=${ARG}

# Some newer Koha catalogs (post-2016) use a new app search syntax.
# For maximum flexibility we'll make everything before the search
# terms included in the base URL (which will usually be something like
# http://my.library.url/app/search/ )
CATTYPE koha-app
SUBURL ${BASEURL}subject:${ARG}
AUTURL ${BASEURL}author:${ARG}
TITURL ${BASEURL}title:${ARG}
KEYURL ${BASEURL}${ARG}

CATTYPE koha-consortium-member
SUBURL ${BASEURL}/cgi-bin/koha/opac-search.pl?multibranchlimit=${LCODE}&idx=su&q=${ARG}
AUTURL ${BASEURL}/cgi-bin/koha/opac-search.pl?multibranchlimit=${LCODE}&idx=au&q=${ARG}
TITURL ${BASEURL}/cgi-bin/koha/opac-search.pl?multibranchlimit=${LCODE}&idx=ti&q=${ARG}
KEYURL ${BASEURL}/cgi-bin/koha/opac-search.pl?multibranchlimit=${LCODE}&q=${ARG}

# some Koha installs seem to use a different syntax for branch limits
CATTYPE koha-group
SUBURL ${BASEURL}/cgi-bin/koha/opac-search.pl?branch_group_limit=branch:${LCODE}&idx=su&q=${ARG}
AUTURL ${BASEURL}/cgi-bin/koha/opac-search.pl?branch_group_limit=branch:${LCODE}&idx=au&q=${ARG}
TITURL ${BASEURL}/cgi-bin/koha/opac-search.pl?branch_group_limit=branch:${LCODE}&idx=ti&q=${ARG}
KEYURL ${BASEURL}/cgi-bin/koha/opac-search.pl?branch_group_limit=branch:${LCODE}&q=${ARG}

# Non-working libero catalog pattern here; unfortunately I haven't
# yet found a way to make Libero searches work without a recent dynamically
# supplied TOKEN argument.  Example of a search URL is below.
# http://ausport.hosting.libero.com.au/libero/WebOpac.cls?VERSION=2&amp;ACTION=SEARCH&amp;RSN=&amp;DATA=ASC&amp;TOKEN=lTpgKekGh62508&amp;Z=1&amp;NotSearch=1&amp;USE_1=ku&amp;TERM_1=ATHLETICS
CATTYPE libero
KINDEX ku
AINDEX kb
TINDEX k
SINDEX sk
TOKEN lTpgKekGh6250
SUBURL ${BASEURL}?VERSION=2&ACTION=SEARCH&DATA=ASC&TOKEN=${TOKEN}&Z=1&NotSearch=1&USE_1=${SINDEX}&TERM_1=${ARG}
AUTURL ${BASEURL}?VERSION=2&ACTION=SEARCH&DATA=ASC&TOKEN=${TOKEN}&Z=1&NotSearch=1&USE_1=${AINDEX}&TERM_1=${ARG}
TITURL ${BASEURL}?VERSION=2&ACTION=SEARCH&DATA=ASC&TOKEN=${TOKEN}&Z=1&NotSearch=1&USE_1=${TINDEX}&TERM_1=${ARG}
KEYURL ${BASEURL}?VERSION=2&ACTION=SEARCH&DATA=ASC&TOKEN=${TOKEN}&Z=1&NotSearch=1&USE_1=${KINDEX}&TERM_1=${ARG}

CATTYPE ls2pac
SUBURL ${BASEURL}?section=search&term=su:${ARG:N}
AUTURL ${BASEURL}?section=search&term=au:${ARG:N}
TITURL ${BASEURL}?section=search&term=ti:${ARG:N}
KEYURL ${BASEURL}?section=search&term=${ARG:N}

CATTYPE ls2pac-config
CONFIG pac
SUBURL ${BASEURL}?config=${CONFIG}#section=search&term=subject:${ARG:NK}
AUTURL ${BASEURL}?config=${CONFIG}#section=search&term=author:${ARG:SNK}
TITURL ${BASEURL}?config=${CONFIG}#section=search&term=title:${ARG:NK}
KEYURL ${BASEURL}?config=${CONFIG}#section=search&term=${ARG:NK}

# same as above but using ampersand as subject
CATTYPE ls2pac-configamp
CONFIG pac
SUBURL ${BASEURL}?config=${CONFIG}&section=search&term=subject:${ARG:NK}
AUTURL ${BASEURL}?config=${CONFIG}&section=search&term=author:${ARG:SNK}
TITURL ${BASEURL}?config=${CONFIG}&section=search&term=title:${ARG:NK}
KEYURL ${BASEURL}?config=${CONFIG}&section=search&term=${ARG:NK}


# Olib from OCLC UK - we only know how to do a keyword search on it so far
CATTYPE olib
KEYURL ${BASEURL}/webview/?keyword=${ARG:NK}

# Polaris seems to time out eventually if you try these kinds of links
# so I'm deprecating this (but keeping it if I need it later)
# CATTYPE polaris-old
# CONTEXT 1.1033.0.0.1
# SUBURL ${BASEURL}/search/searchresults.aspx?type=Keyword&term=${ARG}&by=SU&sort=RELEVANCE
# AUTURL ${BASEURL}/search/searchresults.aspx?type=Keyword&term=${ARG}&by=AU&sort=RELEVANCE
# TITURL ${BASEURL}/search/browse.aspx?ctx=1.1033.0.0.6&type=Browse&term=${ARG}&by=TI&sort=RELEVANCE
# TITURL ${BASEURL}/search/searchresults.aspx?type=Keyword&term=${ARG}&by=TI&sort=RELEVANCE
# KEYURL ${BASEURL}/search/searchresults.aspx?type=Keyword&term=${ARG}&by=KW&sort=RELEVANCE

# Polaris view.aspx seems to work better for deep links
CATTYPE polaris
SUBURL ${BASEURL}/view.aspx?subject=${ARG}
AUTURL ${BASEURL}/view.aspx?author=${ARG}
TITURL ${BASEURL}/view.aspx?title=${ARG}
KEYURL ${BASEURL}/view.aspx?keyword=${ARG}

# Polaris with context; useful when 1 server handles multiple libraries
CATTYPE polaris-context
SUBURL ${BASEURL}/view.aspx?ctx=${CONTEXT}&subject=${ARG}
AUTURL ${BASEURL}/view.aspx?ctx=${CONTEXT}&author=${ARG}
TITURL ${BASEURL}/view.aspx?ctx=${CONTEXT}&title=${ARG}
KEYURL ${BASEURL}/view.aspx?ctx=${CONTEXT}&keyword=${ARG}

# Primo-syntax searches where we need to provide otherr args in baseurl
CATTYPE primo-justquery
SUBURL ${BASEURL}query=sub,contains,${ARG:NK}
AUTURL ${BASEURL}query=creator,contains,${ARG:NK}
TITURL ${BASEURL}query=title,contains,${ARG:NK}
ATIURL ${BASEURL}query=creator,contains,${AUTHOR:NK}&query=title,contains,${TITLE:NK}
KEYURL ${BASEURL}query=any,contains,${ARG:NK}
DEFAULT DOMAIN

# New Primo VE syntax described at https://knowledge.exlibrisgroup.com/Primo/Product_Documentation/020Primo_VE/050Other_Configuration/Configuring_Redirection_for_Primo_VE_Deep_Links
# Not clear if author+title search works as before, so not including
CATTYPE primo-ve
SCOPE EVERYTHING
SUBURL ${BASEURL}/discovery/search?vid=${VID}&search_scope=${SCOPE}&query=sub,contains,${ARG:NK}
AUTURL ${BASEURL}/discovery/search?vid=${VID}&search_scope=${SCOPE}&query=creator,contains,${ARG:NK}
TITURL ${BASEURL}/discovery/search?vid=${VID}&search_scope=${SCOPE}&query=title,contains,${ARG:NK}
KEYURL ${BASEURL}/discovery/search?vid=${VID}&search_scope=${SCOPE}&query=any,contains,${ARG:NK}
DEFAULT DOMAIN

