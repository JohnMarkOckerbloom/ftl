AUTURL ${BASEURL}/Search/Results?type=Author&lookfor=${ARG}
TITURL ${BASEURL}/Search/Results?type=Title&lookfor=${ARG}
KEYURL ${BASEURL}/Search/Results?type=AllFields&lookfor=${ARG}
DEFAULT DOMAIN

CATTYPE vufindnoaccent
