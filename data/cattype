AUTURL ${BASEURL}/search?q=AuthorCombined:(${ARG})
TITURL ${BASEURL}/search?q=TitleCombined:(${ARG})
ATIURL ${BASEURL}/search?q=AuthorCombined:(${AUTHOR})+AND+TitleCombined:(${TITLE})
KEYURL ${BASEURL}/search?q=${ARG}

