CATTYPE voyager
TITURL ${BASEURL}?Search_Arg=${ARG:A}&SL=None&Search_Code=TALL&CNT=25
AUTURL ${BASEURL}?Search_Arg=${ARG}&SL=None&Search_Code=NAME&CNT=25
SUBURL ${BASEURL}?Search_Arg=${ARG}&SL=None&Search_Code=SUBJ&CNT=25
KEYURL ${BASEURL}?Search_Arg=${ARG}&SL=None&Search_Code=GKEY^*&CNT=25
# KEYURL ${BASEURL}?DB=local&FLD1=${ARG:N}&CNT=25

CATTYPE voyager-vwebv
# SUBURL ${BASEURL}?searchArg=${ARG:A}&searchCode=SUBJ&searchType=1&recCount=50
SUBURL ${BASEURL}?searchArg=${ARG:A}&searchCode=SUBJ&searchType=1
AUTURL ${BASEURL}?searchArg=${ARG:A}&searchCode=NAME&searchType=1
TITURL ${BASEURL}?searchArg=${ARG:A}&searchCode=TALL&searchType=1
KEYURL ${BASEURL}?searchArg=${ARG}&searchCode=GKEY^*&searchType=1

# This is for members of a coalition using the catalog
# We do more keyword searching in this version, since browse doesn't filter
#  and the catalog as a whole seems to be more sensitive in this mode
CATTYPE voyager-vwebv-member
SUBURL ${BASEURL}?searchArg=${ARG:NK}&searchCode=SKEY^&searchType=1&limitTo=${LCODE}
AUTURL ${BASEURL}?searchArg=${ARG:S}&searchCode=NKEY*&searchType=1&limitTo=${LCODE}
TITURL ${BASEURL}?searchArg=${ARG:A}&searchCode=TALL&searchType=1&limitTo=${LCODE}
KEYURL ${BASEURL}?searchArg=${ARG}&searchCode=GKEY^*&searchType=1&limitTo=${LCODE}


CATTYPE vubis
DBID 1
KINDEX 1*Keywordsbib
SINDEX 1*Subjectbib$
AINDEX 1*authorbib$
TINDEX 1*Titlebib
PROFILE Default
SUBURL ${BASEURL}/List.csp?Database=${DBID}&OpacLanguage=eng&SearchMethod=Find_1&NumberToRetrieve=50&Profile=${PROFILE}&PreviousList=Start&PageType=Start&WebPageNr=1&WebAction=NewSearch&StartValue=1&RowRepeat=0&Index1=${SINDEX}&SearchT1=${ARG}
AUTURL ${BASEURL}/List.csp?Database=${DBID}&OpacLanguage=eng&SearchMethod=Find_1&NumberToRetrieve=50&Profile=${PROFILE}&PreviousList=Start&PageType=Start&WebPageNr=1&WebAction=NewSearch&StartValue=1&RowRepeat=0&Index1=${AINDEX}&SearchT1=${ARG}
TITURL ${BASEURL}/List.csp?Database=${DBID}&OpacLanguage=eng&SearchMethod=Find_1&NumberToRetrieve=50&Profile=${PROFILE}&PreviousList=Start&PageType=Start&WebPageNr=1&WebAction=NewSearch&StartValue=1&RowRepeat=0&Index1=${TINDEX}&SearchT1=${ARG}
KEYURL ${BASEURL}/List.csp?Database=${DBID}&OpacLanguage=eng&SearchMethod=Find_1&NumberToRetrieve=50&Profile=${PROFILE}&PreviousList=Start&PageType=Start&WebPageNr=1&WebAction=NewSearch&StartValue=1&RowRepeat=0&Index1=${KINDEX}&SearchT1=${ARG}

