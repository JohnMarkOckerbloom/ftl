AUTURL ${BASEURL}/client/default/search/results?&rt=AUTHOR|||Author|||false&qu="${ARG}"
TITURL ${BASEURL}/client/default/search/results?&rt=TITLE|||Title|||false&qu="${ARG}"
KEYURL ${BASEURL}/client/default/search/results?qu=${ARG}

CATTYPE spydus
