# Polaris seems to get confused if you try a browse after a search,
# so these are all keyword searches for now
CATTYPE polaris
CONTEXT 1.1033.0.0.1
SUBURL ${BASEURL}/search/searchresults.aspx?type=Keyword&term=${ARG}&by=SU&sort=RELEVANCE
AUTURL ${BASEURL}/search/searchresults.aspx?type=Keyword&term=${ARG}&by=AU&sort=RELEVANCE
# TITURL ${BASEURL}/search/browse.aspx?ctx=1.1033.0.0.6&type=Browse&term=${ARG}&by=TI&sort=RELEVANCE
TITURL ${BASEURL}/search/searchresults.aspx?type=Keyword&term=${ARG}&by=TI&sort=RELEVANCE
KEYURL ${BASEURL}/search/searchresults.aspx?type=Keyword&term=${ARG}&by=KW&sort=RELEVANCE

# Old primo search; does not seem to work on any current site I've seen
CATTYPE primo-old
KEYURL ${BASEURL}/libweb/action/search.do?mode=Basic&dum=true&operation=1&amp;ct=search&indx=1&vid=${VID}&vl%28freeText0%29=${ARG:NK}&fn=search
DEFAULT DOMAIN

# Primo deep link syntax described at
# https://developers.exlibrisgroup.com/primo/apis/deeplinks
CATTYPE primodl
LCODE PRIMO
SUBURL ${BASEURL}/primo_library/libweb/action/dlSearch.do?vid=${VID}&institution=${LCODE}&query=sub,contains,${ARG:NK}
AUTURL ${BASEURL}/primo_library/libweb/action/dlSearch.do?vid=${VID}&institution=${LCODE}&query=creator,contains,${ARG:NK}
TITURL ${BASEURL}/primo_library/libweb/action/dlSearch.do?vid=${VID}&institution=${LCODE}&query=title,contains,${ARG:NK}
ATIURL ${BASEURL}/primo_library/libweb/action/dlSearch.do?vid=${VID}&institution=${LCODE}&query=creator,contains,${AUTHOR:NK}&query=title,contains,${TITLE:NK}
KEYURL ${BASEURL}/primo_library/libweb/action/dlSearch.do?vid=${VID}&institution=${LCODE}&query=any,contains,${ARG:NK}
DEFAULT DOMAIN

# This is for Primo institutions that seems to require a LOC
# for deep links to work
# (but it's better to use search_scope if needed and available)
CATTYPE primodl-loc
LCODE PRIMO
SUBURL ${BASEURL}/primo_library/libweb/action/dlSearch.do?vid=${VID}&institution=${LCODE}&loc=${LOC}&query=sub,contains,${ARG:NK}
AUTURL ${BASEURL}/primo_library/libweb/action/dlSearch.do?vid=${VID}&institution=${LCODE}&loc=${LOC}&query=creator,contains,${ARG:NK}
TITURL ${BASEURL}/primo_library/libweb/action/dlSearch.do?vid=${VID}&institution=${LCODE}&loc=${LOC}&query=title,contains,${ARG:NK}
ATIURL ${BASEURL}/primo_library/libweb/action/dlSearch.do?vid=${VID}&institution=${LCODE}&loc=${LOC}&query=creator,contains,${AUTHOR:NK}&query=title,contains,${TITLE:NK}
KEYURL ${BASEURL}/primo_library/libweb/action/dlSearch.do?vid=${VID}&institution=${LCODE}&loc=${LOC}&query=any,contains,${ARG:NK}
DEFAULT DOMAIN

# This is for Primo institutions that seems to require a search_scope
# for deep links to work (preferable to LOC)
CATTYPE primodl-scope
LCODE PRIMO
SUBURL ${BASEURL}/primo_library/libweb/action/dlSearch.do?vid=${VID}&institution=${LCODE}&search_scope=${SCOPE}&query=sub,contains,${ARG:NK}
AUTURL ${BASEURL}/primo_library/libweb/action/dlSearch.do?vid=${VID}&institution=${LCODE}&search_scope=${SCOPE}&query=creator,contains,${ARG:NK}
TITURL ${BASEURL}/primo_library/libweb/action/dlSearch.do?vid=${VID}&institution=${LCODE}&search_scope=${SCOPE}&query=title,contains,${ARG:NK}
ATIURL ${BASEURL}/primo_library/libweb/action/dlSearch.do?vid=${VID}&institution=${LCODE}&search_scope=${SCOPE}&query=creator,contains,${AUTHOR:NK}&query=title,contains,${TITLE:NK}
KEYURL ${BASEURL}/primo_library/libweb/action/dlSearch.do?vid=${VID}&institution=${LCODE}&search_scope=${SCOPE}&query=any,contains,${ARG:NK}
DEFAULT DOMAIN

