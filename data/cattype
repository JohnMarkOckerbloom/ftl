TITURL ${BASEURL}/opac/search;jsessionid=?max=&view=CONTENT&level=all&material_type=all&location=&lang=eng&q=all_titles%3A${ARG}
KEYURL ${BASEURL}/opac/search;jsessionid=?max=&view=CONTENT&level=all&material_type=all&location=&lang=eng&q=${ARG}

